      "@icons/*":["src/assets/icons/*"],
      "@utils/*":["src/utils/*"],
      "@plugins/*":["src/plugins"],
      "@styles/*":["src/assets/styles"],
      "@fonts/*":["src/assets/fonts"]
    }
  }
}
